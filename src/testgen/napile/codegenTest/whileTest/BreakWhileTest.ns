class BreakWhileTest
{
	static meth main(val arg : Array<String>)
	{
		var b = 0

		while (true)
		{
			if(b == 10)
			{
				Logger.System.info("10 break")
				break
			}

			Logger.System.info(b.toString())

			b = b + 1
		}

		Logger.System.info("End")
	}
}