class ContinueTest
{
	static meth main(val arg : Array<String>)
	{
		var b = 0

		while (b != 10)
		{
			b ++

			if(b % 2 == 0)
			{
				Console.writeLine(b.toString())
				continue
			}
		}

		Console.writeLine("End")
		return
	}
}