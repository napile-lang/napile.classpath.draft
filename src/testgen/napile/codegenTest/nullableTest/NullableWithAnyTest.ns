package codegenTest.nullableTest

import napile.io.Console

/**
 * @author VISTALL
 */
class NullableWithAnyTest
{
	static meth main(val arg : Array<String>)
	{
		var a : MyObject = MyObject()

		var b : MyObject? = null
		var c : MyObject? = a

		if(b == null)
			Console.writeLine("b is null")

		if (c != null)
			Console.writeLine("c is not null")

		if(c.equals(a))
			Console.writeLine("a is fullyEquals c")
	}
}