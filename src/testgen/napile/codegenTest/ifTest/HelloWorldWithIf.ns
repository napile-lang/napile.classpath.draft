class HelloWorldWithIf
{
	static meth main(val arg : Array<String>)
	{
		var b = false

		if(!b)
			Logger.System.info("Hello world!!! : FALSE");

		Logger.System.info("Hello world!!!");

		b = true
		if(b)
			Logger.System.info("Hello world!!! : TRUE");
	}
}