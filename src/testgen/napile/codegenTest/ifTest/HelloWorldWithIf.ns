class HelloWorldWithIf
{
	static meth main(val arg : Array<String>)
	{
		var b = false

		if(!b)
			Console.writeLine("Hello world!!! : FALSE");

		Console.writeLine("Hello world!!!");

		b = true
		if(b)
			Console.writeLine("Hello world!!! : TRUE");
	}
}