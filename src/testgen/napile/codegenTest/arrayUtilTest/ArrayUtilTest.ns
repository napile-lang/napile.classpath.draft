class ArrayUtilTest
{
	static meth main(val arg : Array<String>)
	{
		var b = Array<String>(10)
		var i = 0
		for (c in b)
			b[i++] = i.toString()

		for (c in b)
		{
			Console.write(c)
			Console.write("-")
		}

		Console.writeLine("")
		Console.writeLine("After copyOfRange")

		var newArray = ArrayUtil.copyOfRange(b, 0, 5)
		for (val c in newArray)
		{
			Console.write(c)
			Console.write("-")
		}
	}
}