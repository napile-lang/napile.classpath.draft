class MultiLabelTest
{
	static meth main(val arg : Array<String>)
	{
		label A
		{
			label B
			{
				if (Bool.TRUE)
				{
					Console.writeLine("break B")
					break B
				}
				Console.writeLine("UnEnter code 'B'")
			}

			Console.writeLine("Enter code 'A'")
		}
		return
	}
}