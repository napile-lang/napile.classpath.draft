class Test
{
	static var ENTRY : [var key : String, var value : String]

	static meth main(val arg : Array<String>)
	{
		ENTRY = ["Key", "Value"]

		Console.writeLine("key is " + ENTRY.key)
		Console.writeLine("value is " + ENTRY.value)

		ENTRY.key = "NewKey"
		ENTRY.value = "NewValue"

		Console.writeLine("key is " + ENTRY.key)
		Console.writeLine("value is " + ENTRY.value)
	}
}