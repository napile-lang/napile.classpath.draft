class WhenTest1
{
	this()

	meth test1()
	{
		var c : Any = 1

		Console.writeLine(c.equals(1).toString())

		when(c)
		{
			1 -> c = 2
			else -> c = 3
		}

		Console.writeLine(c.toString())
	}

	static meth main(val arg : Array<String>)
	{
		WhenTest1().test1()
	}
}