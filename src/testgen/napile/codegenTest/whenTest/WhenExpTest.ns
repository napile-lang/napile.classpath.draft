package codegenTest.whenTest

import napile.logging.Logger

/**
 * @author VISTALL
 */
class WhenExpTest
{
	static meth main(val arg : Array<String>)
	{
		val i : Any = 1

		when
		{
			i == 2 -> Logger.System.info("i == 2")
			i == 1 -> Logger.System.info("i == 1")
			else -> Logger.System.info("error")
		}
	}
}