class WhenTest
{
	static meth main(val arg : Array<String>)
	{
		var i = 1

		when(i)
		{
			1 -> Console.writeLine("i == 1")
			2 -> Console.writeLine("i == 2")
			else -> Console.writeLine("i - else")
		}
	}
}