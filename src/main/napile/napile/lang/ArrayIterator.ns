package napile.lang

import napile.io.Console
import napile.collection.Iterator

/**
 * @author VISTALL
 */
class ArrayIterator<E> : Iterator<E>
{
	local var index : Int = 0
	local var array : Array<E>

	this(array) : Iterator<E>()

	override meth next() : E
	{
		var val = array[index];
		index = index + 1
		return val
	}

	override meth hasNext() : Bool
	{
		return index != array.length
	}
}