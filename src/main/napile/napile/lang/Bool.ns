package napile.lang

import napile.annotation.Enumable

/**
 * @author VISTALL
 */
class Bool
{
	//@Enumable
	static final var TRUE : Bool = Bool("true")

	//@Enumable
	static final var FALSE : Bool = Bool("false")

	local var value : String

	local this(value)

	meth not() : Bool = if(this) FALSE else TRUE

	override meth toString() = value
}