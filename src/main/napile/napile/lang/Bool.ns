package napile.lang

import napile.annotation.Enumable

/**
 * @author VISTALL
 */
final class Bool
{
	//@Enumable
	static val TRUE : Bool = Bool()

	//@Enumable
	static val FALSE : Bool = Bool()

	local this()

	meth not() : Bool = if(this) FALSE else TRUE

	override meth toString() = if(this == TRUE) "true" else "false"
}