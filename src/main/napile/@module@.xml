<?xml version="1.0" encoding="UTF-8"?>

<module>
	<id>napile.lang</id>
	<name>napile : lang</name>
	<build-by>@build-by@</build-by>
	<version>1.0</version>

	<extension-points>
		<extension-point name="loggingHandler" descriptor="napile.logging.LoggingHandlerDescriptor">
			<ref-attribute-to-type-parameter attribute="impl" type-parameter="T" />
		</extension-point>
	</extension-points>

	<extensions for-id="napile.lang">
		<loggingHandler id="System" impl="napile.logging.handlers.ConsoleLoggingHandler" />
	</extensions>
</module>