package codegenTest.nullableTest

import napile.io.Console

/**
 * @author VISTALL
 */
class SureTest
{
	static meth main(arg : Array<String>)
	{
		try
		{
			var myVar : String? = null

			Console.writeLine(myVar!!)
		}
		catch(e : NullPointerException)
		{
			e.printStackTrace()
		}

		Console.writeLine("Test is ended");
	}
}