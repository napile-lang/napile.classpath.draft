package codegenTest.typeParameterTest

import napile.io.Console

/**
 * @author VISTALL
 */
class TWithConstructorTest
{
	static meth factory<E(value : Int)>(value : Int) : E  = E(value)

	static meth main(arg : Array<String>)
	{
		var c : TWithTarget = factory(1234)

		Console.writeLine("Class is: " + c.getClass().name + ", length: " + c.length)

		c = factory(1)

		Console.writeLine("Class is: " + c.getClass().name + ", length: " + c.length)
	}
}