class DoWhileTest
{
	static meth main(arg : Array<String>)
	{
		var b = 0
		do
		{
			if (b == 0)
				Console.writeLine("b = 0")
			if (b == 1)
				Console.writeLine("b = 1")
			if (b == 2)
				Console.writeLine("b = 2")
			if (b == 3)
				Console.writeLine("b = 3")
			b = b + 1
		}
		while (b != 3)

		Console.writeLine("End")
		return
	}
}