class LazyPropertyTest
{
	var value : String

	this()

	meth value.lazy() : String
	{
		Console.writeLine("lazy call")
		return "LAZY_VALUE"
	}

	static meth main(arg : Array<String>)
	{
		var b = LazyPropertyTest()

		for (i in 0..10)
			Console.writeLine(b.value)
	}
}