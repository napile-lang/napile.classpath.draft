class BreakWhileTest
{
	static meth main(arg : Array<String>)
	{
		var b = 0

		while (true)
		{
			if(b == 10)
			{
				Console.writeLine("10 break")
				break
			}

			Console.writeLine(b.toString())

			b = b + 1
		}

		Console.writeLine("End")
		return
	}
}