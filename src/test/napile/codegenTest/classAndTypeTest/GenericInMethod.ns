class GenericInMethod
{
	this()

	meth me<E>(val : E) : E
	{
		Console.writeLine(classOf(E).name);
		return val
	}

	static meth main(arg : Array<String>)
	{
		var o = GenericInMethod()

		var b = o.me/*<Int>*/(1)

		Console.writeLine(b.getClass().name)
	}
}